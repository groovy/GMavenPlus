<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/DECORATION/1.8.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.8.0 http://maven.apache.org/xsd/decoration-1.8.0.xsd"
         name="GMavenPlus Plugin">

  <bannerLeft>
    <name>GMavenPlus</name>
    <src>/images/logos/GMavenPlusLogo_sm.png</src>
    <alt>GMavenPlus logo</alt>
    <href>/index.html</href>
  </bannerLeft>

  <skin>
    <groupId>org.apache.maven.skins</groupId>
    <artifactId>maven-fluido-skin</artifactId>
    <version>2.0.0-M11</version>
  </skin>

  <custom>
    <fluidoSkin>
      <topBarEnabled>false</topBarEnabled>
      <sideBarEnabled>true</sideBarEnabled>
      <gitHub>
        <projectId>groovy/gmavenplus</projectId>
        <ribbonOrientation>right</ribbonOrientation>
        <ribbonColor>red</ribbonColor>
      </gitHub>
      <googleSearch />
    </fluidoSkin>
  </custom>

  <body>
    <menu name="Overview">
      <item name="Introduction" href="index.html"/>
      <item name="Goals" href="plugin-info.html"/>
      <item name="Usage" href="https://github.com/groovy/GMavenPlus/wiki/Usage"/>
      <item name="Examples" href="https://github.com/groovy/GMavenPlus/wiki/Examples"/>
    </menu>
    <menu ref="reports"/>
  </body>

</project>
